{
  "name": "umi-template",
  "version": "0.0.1",
  "description": "Template for umi based project",
  "scripts": {
    "start": "UMI_ENV=local umi dev",
    "build:staging": "UMI_ENV=stage BABEL_POLYFILL=none umi build",
    "build:master": "UMI_ENV=prod BABEL_POLYFILL=none umi build",
    "prepare": "husky install",
    "lint:ls": "ls-lint",
    "analyze": "ANALYZE=1 UMI_ENV=stage umi build",
    "prettier": "prettier --write 'src/**' 'server/**'"
  },
  "license": "MIT",
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@ls-lint/ls-lint": "^1.9.2",
    "@types/classnames": "^2.2.11",
    "@types/faker": "^5.1.7",
    "@types/jest": "^26.0.20",
    "@types/js-cookie": "^2.2.6",
    "@types/webpack-env": "^1.16.0",
    "@umijs/plugin-access": "^2.3.3",
    "@umijs/plugin-antd": "^0.9.1",
    "@umijs/plugin-initial-state": "^2.3.0",
    "@umijs/plugin-model": "^2.5.6",
    "@umijs/plugin-request": "^2.5.2",
    "@umijs/test": "^3.4.0",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "faker": "^5.4.0",
    "husky": "^6.0.0",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^10.5.4",
    "loader-utils": "^2.0.0",
    "prettier": "^2.3.0",
    "react-coding-style": "^1.3.1",
    "tslib": "^2.2.0",
    "typescript-plugin-css-modules": "^3.3.0",
    "umi": "^3.4.0"
  },
  "dependencies": {
    "ahooks": "^2.9.6",
    "classnames": "^2.2.6",
    "dequal": "^2.0.2",
    "js-cookie": "^2.2.1"
  },
  "engines": {
    "node": "^12"
  }
}
